{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 25,
      "MaxConcurrentUpgradedConnections": 25
    },
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    },
    "DisableStringReuse": true
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "WebAppContext": "Server=(localdb)\\mssqllocaldb;Database=WebApp.Data;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}